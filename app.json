{
  "expo": {
    "name": "EffnerApp",
    "slug": "EffnerApp",
    "version": "3.1.1",
    "orientation": "default",
    "entryPoint": "index.js",
    "icon": "./@resources/effnerapp_logo.png",
    "splash": {
      "image": "./@resources/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "48",
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "de.effnerapp.effner",
      "supportsTablet": true
    },
    "android": {
      "package": "de.effnerapp.effner",
      "versionCode": 100,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./@resources/ic_launcher_foreground.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./@resources/effnerapp_logo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./@resources/effnerapp_push_icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
