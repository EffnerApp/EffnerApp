{
  "expo": {
    "name": "EffnerApp",
    "slug": "EffnerApp",
    "version": "3.3.1",
    "orientation": "default",
    "entryPoint": "index.js",
    "icon": "./@resources/default_icon.png",
    "splash": {
      "image": "./@resources/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "68",
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "de.effnerapp.effner",
      "supportsTablet": true
    },
    "android": {
      "package": "de.effnerapp.effner",
      "versionCode": 122,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./@resources/android_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./@resources/default_icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./@resources/push_icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "./plugins/androidConfig.js",
        {
          "compileSdkVersion": 31,
          "targetSdkVersion": 31
        }
      ],
      [ 
        "./plugins/androidManifestConfig.js"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "906a3c44-1378-4fe9-8633-34076d1f1fe1"
      }
    }
  }
}
