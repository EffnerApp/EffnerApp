{
  "expo": {
    "name": "EffnerApp",
    "slug": "EffnerApp",
    "version": "3.1.2",
    "orientation": "default",
    "entryPoint": "index.js",
    "icon": "./@resources/default_icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "49",
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "de.effnerapp.effner",
      "supportsTablet": true,
      "splash": {
        "image": "./@resources/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#121212"
      }
    },
    "android": {
      "package": "de.effnerapp.effner",
      "versionCode": 101,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./@resources/android_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./@resources/default_icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./@resources/push_icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
